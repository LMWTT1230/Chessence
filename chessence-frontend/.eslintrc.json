{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "no-unused-vars": "warn",
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off",
        "prettier/prettier": "warn"
    }
}
